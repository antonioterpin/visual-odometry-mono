{
        "InputBlock": {
                "Handler": "Kitti",
                "Path": "data/kitti"
        },
        "InitBlock": {
                "Handler": "PatchMatching",
                "Params" : {
                        "RANSACIt": 2000,
                        "verbose": true,
                        "nIt": 2,
                        "nSkip": 1,
                        "adaptive": 0.99,
                        "stopWithNPoints": 90,
                        "errorThreshold": 1,
                        "maxDistance": 300
                }
        },
        "COBlock": {
                "Handler": "P3PRANSAC",
                "Params": {
                        "p3pRANSACIt": 2000,
                        "p3pTolerance": 1,
                        "adaptive": 0.99,
                        "minInliers": 5
                }
        },
        "OptBlock": {},
        "OutputBlock": {
                "superimposeGT": false,
                "recentTrajectorySize": 15,
                "landmarksHistorySizePlot": 15,
                "smooth": false
        },
        "DetectorBlock": {
                "Handler": "Harris",
                "Params": {
                        "patchSize": 11,
                        "kappa": 0.08,
                        "nKeypoints": 2000,
                        "nonMaximaSuppressionRadius": 5,
                        "descriptorRadius": 11,
                        "lambda": 3
                }
        },
        "Pipeline": {
            "verbose": true,
            "nSkip": 1,
            "startingFrame": 80,
            "lastFrame": 550,
            "State": {
                "lostBelow": 15,
                "verbose": true,
                "candidatesWindowSize": 2,
                "cosineThreshold": 0.9
            },
            "continuouslyTriangulate": false
        }
}